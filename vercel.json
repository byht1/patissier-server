{
  "version": 1,
  "env": {
    "DB_HOST": "@mongo_uri",
    "SECRET_KEY": "@secret_key",
    "EMAIL_USER": "@email_user",
    "EMAIL_PASSWORD": "@email_password"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
